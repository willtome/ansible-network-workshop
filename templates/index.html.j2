  </div>
{% for item in users %}
  <div data-role="main" class="ui-content">
    <div data-role="collapsible">
      <h1>{{item.username}} - Click to see details!</h1>
      <p>[all:vars]<br />
ansible_user={{item.username}}<br />
ansible_ssh_pass=ansible<br />
{% if ssh_port is defined %}<br />
ansible_port={{ ssh_port }}<br />
{% endif %}
<br />
{% set exculded_groups = ('all', 'ungrouped', 'managed_nodes', 'public', 'private') %}
{% for group in groups %}
{% if group not in ('all', 'ungrouped', 'managed_nodes', 'public', 'private') %}
[{{group}}]<br />
{% for vm in ec2_instances.instances %}
{% if item.username == vm.tags.Owner and group == vm.tags.group %}
{{ vm.tags.Role }} ansible_host={{ hostvars[vm.tags.Name]['ansible_host'] }}<br />
{% endif %}
{% endfor %}
<br />
{% endif %}
{% endfor %}
</p>
    </div>
  </div>
{% endfor %}
  <div data-role="footer">
    <h1>Thanks for participating!</h1>
  </div>
</div>

</body>
